{
  "name": "fetch-the-standard-news",
  "author": "Theerut Top",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://topkoong.github.io/fetch-the-standard-news",
  "keywords": [
    "react",
    "vite",
    "typescript",
    "ghactions",
    "vitest"
  ],
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write src/**/*.ts{,x}",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "lint": "yarn lint:format && yarn lint:fix ",
    "type-check": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/topkoong/fetch-the-standard-news.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/topkoong/fetch-the-standard-news/issues"
  },
  "dependencies": {
    "preact": "latest",
    "react-query": "^3.39.2",
    "react-router-dom": "6"
  },
  "devDependencies": {
    "@preact/preset-vite": "latest",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "latest",
    "autoprefixer": "^10.4.8",
    "axios": "^0.27.2",
    "eslint": "latest",
    "eslint-config-preact": "latest",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.8",
    "typescript": "latest",
    "vite": "latest",
    "vite-tsconfig-paths": "^3.5.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,md,json}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint"
    }
  }
}
