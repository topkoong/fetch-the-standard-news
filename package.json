    "lint": "yarn lint:format && yarn lint:fix ",
    "preview": "vite preview"
  },
  "dependencies": {
    "preact": "latest"
  },
  "devDependencies": {
    "@preact/preset-vite": "latest",
    "@typescript-eslint/parser": "latest",
    "autoprefixer": "^10.4.8",
    "axios": "^0.27.2",
    "eslint": "latest",
    "eslint-config-preact": "latest",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.8",
    "typescript": "latest",
    "vite": "latest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint"
    }
  }
}
