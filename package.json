{
  "name": "fetch-the-standard-news",
  "author": "Theerut Top",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://topkoong.github.io/fetch-the-standard-news",
  "keywords": [
    "preact",
    "vite",
    "typescript",
    "ghactions",
    "vitest",
    "eslint",
    "prettier",
    "husky",
    "tailwindcss"
  ],
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --fix --ignore-path ./.gitignore",
    "lint:format": "prettier --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "lint": "yarn lint:format && yarn lint:fix ",
    "type-check": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/topkoong/fetch-the-standard-news.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/topkoong/fetch-the-standard-news/issues"
  },
  "dependencies": {
    "preact": "latest",
    "react-query": "latest",
    "react-router-dom": "6"
  },
  "devDependencies": {
    "@preact/preset-vite": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "autoprefixer": "latest",
    "axios": "latest",
    "eslint": "latest",
    "eslint-config-preact": "latest",
    "eslint-config-prettier": "latest",
    "eslint-import-resolver-typescript": "latest",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "husky": "latest",
    "lint-staged": "latest",
    "postcss": "^8.4.20",
    "prettier": "^2.8.4",
    "tailwindcss": "^3.2.6",
    "typescript": "latest",
    "vite": "latest",
    "vite-tsconfig-paths": "latest"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,md,json}": [
      "prettier --loglevel warn --write"
    ],
    "*.{ts,tsx}": "eslint --fix --quiet --fix --ignore-path ./.gitignore"
  }
}
